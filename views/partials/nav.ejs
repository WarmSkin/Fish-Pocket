  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  <nav>
    <img src="/asserts/icons/favicon.png" alt="fish-logo">
    <div class="dropdown">
      <button class="dropbtn">My Page</button>
      <div class="dropdown-content">
        <a href="/users/mypage">My Page</a>
        <a href="/users">All Users</a>
        <a href="/fish">Fish Caught</a>
        <a href="/species">Fish Data</a>
        <% if(user?.profile.level >= 900) {%>
          <a href="/users/maintenance">Maintenance</a>
        <% } %>
      </div>
    </div>
    <% if (user) { %>
      <a href="/auth/logout" class="right">
        <button type="submit" id="googleBtn"><img src="/asserts/icons/door.png" alt="logout-logo" style="width: 40px;"></button>
      </a>
    <% } else { %>
      <div class="dropdown">
        <button class="dropbtn">Sign In</button>
        <div class="dropdown-content">
        <div 
          id="g_id_onload"
          data-client_id="<%= googleClientID %>"
          data-login_uri="/auth/google"
          data-auto_prompt="false"
        ></div>
        <div 
          class="g_id_signin right"
          data-type="regular"
          data-size="large"
          data-theme="outline"
          data-text="sign_in_with"
          data-shape="rectangular"
          data-logo_alignment="left"
        ></div>
      </div>
    </div>
    <% } %>
  </nav>
